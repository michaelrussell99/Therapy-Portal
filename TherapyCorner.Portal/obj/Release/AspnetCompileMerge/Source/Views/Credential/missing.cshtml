@using TherapyCorner.Portal.Models
@using TherapyCorner.Portal.ResourceText;
@using www.therapycorner.com.account;

@model List<MissingCredentialCounts>
@{
    ViewBag.Title = SharedPages.MissingCredentials ;
}

<h2>@SharedPages.MissingCredentials </h2>

<table class="sortable">
    <thead>
        <tr>
            <th class="tdSingleButton"></th>
            <th><a href="#">@Dictionary.Type </a></th>
            <th><a href="#">@Dictionary.Required </a></th>
            <th class="BoolText"><a href="#">@Dictionary.Blocking </a></th>
            <th><a href="#">@Dictionary.Uploaded  </a></th>
            <th><a href="#">@Dictionary.Verified  </a></th>

       </tr>
    </thead>
    <tbody>
        @foreach(var t in Model)
        {
            <tr>
                <td >
                @if (t.Required > t.Uploaded)
                {
                    <a href="@Url.Action("create","credential",new { id = t.TypeId  })"><img src="~/images/add.png" /></a>
                }
                 </td>
                <td>@t.Name </td>
                <td class="NumberText">@t.Required</td>
                <td>@t.Blocking </td>
                <td class="NumberText">@Html.ActionLink(t.Uploaded.ToString(),"index","profile",new { tab = 1 },null)</td>
                <td class="NumberText">@t.Verified</td>
          </tr>
        }
    </tbody>
</table>

